prefix=V:/static-poppler/root-x64
libdir=${prefix}/lib
includedir=${prefix}/include

sysconfdir=V:/static-poppler/root-x64/etc
localstatedir=V:/static-poppler/root-x64/var
confdir=${sysconfdir}/fonts
cachedir=${localstatedir}/cache/fontconfig

Name: Fontconfig
Description: Font configuration and customization library
Version: 2.14.1
Requires: freetype2 >=  21.0.15, expat, libpng
Libs: -L${libdir} -llibfontconfig
Cflags: -I${includedir}
